{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "mobile-host",
  "root": "apps/mobile-host",
  "targets": {
    "dev-sdk": { "executor": "nx:run-commands", "options": { "command": "pnpm -C apps/mobile-host dev-sdk" } },
    "dev-federated": { "executor": "nx:run-commands", "options": { "command": "pnpm -C apps/mobile-host dev-federated" } },
    "build-dev-client-android": { "executor": "nx:run-commands", "options": { "command": "pnpm -C apps/mobile-host build-dev-client-android" } },
    "typecheck": { "executor": "nx:run-commands", "options": { "command": "pnpm -C apps/mobile-host typecheck" } },
    "lint": { "executor": "nx:run-commands", "options": { "command": "pnpm -C apps/mobile-host lint" } }
  },
  "implicitDependencies": ["design-system", "platform-core"]
}
