{
  "name": "@design-system",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./web/*": {
      "import": "./dist/web/*.js",
      "types": "./dist/web/*.d.ts"
    },
    "./native/*": {
      "import": "./dist/native/*.js",
      "types": "./dist/native/*.d.ts"
    },
    "./theme": {
      "import": "./dist/theme/index.js",
      "types": "./dist/theme/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest --root ./"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "@mui/material": ">=6"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@mui/material": "^6.1.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0"
  }
}